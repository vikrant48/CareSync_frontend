# Payment Gateway Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env file with real values to version control

# Razorpay Configuration (Get from https://dashboard.razorpay.com)
RAZORPAY_KEY_ID=rzp_test_your_key_here
RAZORPAY_KEY_SECRET=your_razorpay_secret_here

# Payment Gateway (can be same as Razorpay secret for Razorpay)
PAYMENT_GATEWAY_SECRET_KEY=your_payment_gateway_secret_key_here

# UPI Configuration
MERCHANT_UPI_ID=caresync@paytm

# Webhook Secret (Generate in payment gateway dashboard)
PAYMENT_WEBHOOK_SECRET=your_webhook_secret_here

# Note: Only RAZORPAY_KEY_ID should be used in frontend
# All other secrets should ONLY be used in backend API